{
  "license_path": "/opt/consul/config/consul-ent-license.hclic",
  "acl": {
    "enabled": true,
    "default_policy": "allow",
    "down_policy": "extend-cache",
    "enable_token_persistence": true,
    "tokens": {
      "initial_management": "00000000-0000-0000-0000-000000000000",
      "agent": "00000000-0000-0000-0000-000000000000"
    }
  },
  "connect": {
    "enabled": true
   },
  "auto_encrypt": {
    "allow_tls": true
  },
  "retry_join_wan": [
    "provider=aws region=ap-southeast-2 tag_key=consul-federation-id tag_value=consul-federation-id-value"
  ]
}